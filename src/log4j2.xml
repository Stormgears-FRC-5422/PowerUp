<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %class{36}.%M:%L - %msg%xEx%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<!--<Logger name="org.stormgears.utils.TestDrive" level="trace" />-->

		<Root level="info">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>
